<?xml version="1.0"?>
<config>
	<modules>
		<Compunnel_Prediction>
			<version>1.0.0.0</version>
		</Compunnel_Prediction>
	</modules>
	<global>
		<helpers>
			<prediction>
				<class>Compunnel_Prediction_Helper</class>
			</prediction>
		</helpers>
		<blocks>
			<prediction>
				<class>Compunnel_Prediction_Block</class>
			</prediction>
		</blocks>
	</global>
	<frontend>
		<layout>
			<updates>
				<prediction>
					<file>compunnel/prediction.xml</file>
				</prediction>
			</updates>
		</layout>
		<routers>
			<prediction>
				<use>standard</use>
				<args>
					<module>Compunnel_Prediction</module>
					<frontName>prediction</frontName>
				</args>
			</prediction>
		</routers>
	</frontend>
</config>
